set terminal pngcairo size 640,480 enhanced dash font 'DejaVu Sans,10'
set output 'IMAGE.PNG'

set xrange [0:5e-6*1000000.]
set yrange [-7:+7]

set grid

set style line 10 lc rgb '#0060ad' lt 1 lw 2.5
set style line 11 lc rgb '#60ad00' lt 1 lw 2.5

set format x "%4.0fÂµs"
set format y "%4.0fV"

plot for[n=1:2] "FINAL.CSV" using (column(1)*1000000.):n+1 "%lf,%lf,%lf" \
		    with lines \
		    linestyle 9+n \
		    title sprintf("Channel %d", n)

